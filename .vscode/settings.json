{
    "files.associations": {
        "*.asm": "plaintext",
        ".clang-format": "yaml",
        "SConscript": "python",
        "SConstruct": "python",
        "*.lds": "linkerscript",
        "*.S": "c",
        "*.prelds": "c",
        "*.SCons": "python",
        "*.h": "c",
        "*.s": "c",
        "cstdlib": "c",
        "array": "c",
        "string": "c",
        "string_view": "c",
        "ranges": "c",
        "*.template": "c"
    },
    "C_Cpp.clang_format_path": "${workspaceFolder}/tools/clang-format/clang-format",
    "search.exclude": {
        "output/": true
    },
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.rulers": [
        100
    ],
    "C_Cpp.errorSquiggles": "disabled"
}
